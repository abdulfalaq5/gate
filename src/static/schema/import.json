{
	"SchemaResponseImportTemplate": {
		"type": "object",
		"properties": {
			"headers": {
				"type": "array",
				"items": {
					"type": "string"
				},
				"example": ["Companies", "Departements", "Titles"]
			},
			"sampleData": {
				"type": "array",
				"items": {
					"type": "array",
					"items": {
						"type": "string"
					}
				},
				"example": [
					["PT. Example Company", "Human Resources", "HR Manager"],
					["PT. Example Company", "IT Department", "Software Developer"],
					["PT. Example Company", "Finance", "Accountant"]
				]
			},
			"instructions": {
				"type": "array",
				"items": {
					"type": "string"
				},
				"example": [
					"First column: Company names",
					"Second column: Department names",
					"Third column: Title/Position names",
					"Each row represents a company-department-title relationship",
					"Empty cells will be skipped",
					"Duplicate data will be rejected"
				]
			}
		}
	},
	"SchemaResponseImportResult": {
		"type": "object",
		"properties": {
			"summary": {
				"type": "object",
				"properties": {
					"totalProcessed": {
						"type": "integer"
					},
					"successCount": {
						"type": "integer"
					},
					"errorCount": {
						"type": "integer"
					}
				}
			},
			"details": {
				"type": "array",
				"items": {
					"type": "object",
					"properties": {
						"type": {
							"type": "string",
							"enum": ["company", "department", "title"]
						},
						"name": {
							"type": "string"
						},
						"id": {
							"type": "string",
							"format": "uuid"
						},
						"status": {
							"type": "string",
							"enum": ["created", "error"]
						},
						"error": {
							"type": "string"
						}
					}
				}
			}
		}
	},
	"SchemaResponseImportDuplicate": {
		"type": "object",
		"properties": {
			"type": {
				"type": "string",
				"enum": ["company", "department", "title"]
			},
			"name": {
				"type": "string"
			},
			"message": {
				"type": "string"
			}
		}
	}
}
