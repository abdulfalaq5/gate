{
	"/menus": {
		"get": {
			"tags": [
				"Menus"
			],
			"summary": "Get all menus with filtering, pagination, and sorting",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"parameters": [
				{
					"name": "page",
					"in": "query",
					"required": false,
					"schema": {
						"type": "integer",
						"minimum": 1,
						"default": 1
					},
					"description": "Page number for pagination"
				},
				{
					"name": "limit",
					"in": "query",
					"required": false,
					"schema": {
						"type": "integer",
						"minimum": 1,
						"maximum": 100,
						"default": 10
					},
					"description": "Number of items per page (max 100)"
				},
				{
					"name": "sort_by",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string",
						"enum": ["menu_name", "menu_order", "created_at", "updated_at"],
						"default": "menu_order"
					},
					"description": "Column to sort by"
				},
				{
					"name": "sort_order",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string",
						"enum": ["asc", "desc"],
						"default": "asc"
					},
					"description": "Sort order (asc or desc)"
				},
				{
					"name": "search",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string"
					},
					"description": "Search term for menu_name and menu_url (case-insensitive)"
				},
				{
					"name": "menu_name",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string"
					},
					"description": "Filter by exact menu name"
				},
				{
					"name": "menu_url",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string"
					},
					"description": "Filter by exact menu URL"
				},
				{
					"name": "menu_icon",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string"
					},
					"description": "Filter by exact menu icon"
				},
				{
					"name": "start_date",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string",
						"format": "date"
					},
					"description": "Filter by creation date (from)"
				},
				{
					"name": "end_date",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string",
						"format": "date"
					},
					"description": "Filter by creation date (to)"
				}
			],
			"responses": {
				"200": {
					"description": "OK",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaMenusPaginatedResponse"
							}
						}
					}
				},
				"500": {
					"description": "Internal Server Error",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaErrorResponse"
							}
						}
					}
				}
			}
		},
		"post": {
			"tags": [
				"Menus"
			],
			"summary": "Create new menu",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"requestBody": {
				"required": true,
				"content": {
					"application/json": {
						"schema": {
							"$ref": "#/components/schemas/SchemaCreateMenu"
						}
					}
				}
			},
			"responses": {
				"201": {
					"description": "Created",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaMenusResponse"
							}
						}
					}
				},
				"400": {
					"description": "Bad Request",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaErrorResponse"
							}
						}
					}
				},
				"500": {
					"description": "Internal Server Error",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaErrorResponse"
							}
						}
					}
				}
			}
		}
	},
